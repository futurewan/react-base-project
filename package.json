    "start": "cross-env NODE_ENV=development webpack server --config config/start.js",
    "dev": "cross-env NODE_ENV=development webpack server --config config/start.js",
    "build:prod": "cross-env NODE_ENV=production node config/build.js",
    "build:test": "cross-env NODE_ENV=test node config/build.js",
    "analyze": "cross-env NODE_ENV_REPORT=true npm run build:prod",
    "lint": "eslint src --ext ts,tsx,js,jsx && stylelint \"./src/**/*.scss\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
